model externalids {
    id         Int      @id @default(autoincrement())
    createat   DateTime @default(now())
    updateat   DateTime @updatedAt

    element   Element? @relation(fields: [elementId], references: [id])
    elementId Int     @unique

    imdb_id String
    tmdb_id String
    rttm_id String
    mtcr_id String
}

