model Element {
  id   Int @id @unique @default(autoincrement())
  year Int

  poster   String
  backdrop String

  title          String @unique
  title_original String

  country String

  popularity Float

  genres Genre?

  external_ids externalids?

  category categoryName

  credits  Credits?
  metadata Metadata?
  ratings  Ratings?

  plot     String
  abstract String

  createat   DateTime     @default(now())
  updateat   DateTime     @updatedAt

}
