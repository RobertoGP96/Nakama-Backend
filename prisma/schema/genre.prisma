model Genre {
  id     Int         @id @default(autoincrement())
  genres genreName[]

  element   Element? @relation(fields: [elementId], references: [id])
  elementId Int      @unique
}

enum genreName {
  Accion     @map("Acción")
  Aventura
  Animado
  Comedia
  Crimen
  Documental
  Drama
  Fantacia   @map("Fantasía")
  Historico  @map("Histórico")
  Horror
  Musical
  Misterio
  Romance
  Sci_Fi     @map("Sci-Fi")
  Thriller
  Belica     @map("Bélica")
  Oeste
  Familia
  TV
}
